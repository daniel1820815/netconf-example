<config xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <network-instances xmlns="http://openconfig.net/yang/network-instance">
    <network-instance>
      <name>default</name>
      <config>
        <name>default</name>
      </config>
      <vlans>
        <vlan>
          <vlan-id>30</vlan-id>
          <config>
            <vlan-id>30</vlan-id>
            <status>ACTIVE</status>
          </config>
        </vlan>
      </vlans>
    </network-instance>
  </network-instances>
  <interfaces xmlns="http://openconfig.net/yang/interfaces">
    <interface>
      <name>eth1/1</name>
      <config>
        <name>eth1/1</name>
        <description>To Router1 Gi4</description>
        <enabled>true</enabled>
      </config>
      <ethernet xmlns="http://openconfig.net/yang/interfaces/ethernet">
        <switched-vlan xmlns="http://openconfig.net/yang/vlan">
          <config>
            <interface-mode>ACCESS</interface-mode>
            <access-vlan>30</access-vlan>
          </config>
        </switched-vlan>
      </ethernet>
    </interface>
    <interface>
      <name>eth1/2</name>
      <config>
        <name>eth1/2</name>
        <description>To Router2 Gi4</description>
        <enabled>true</enabled>
      </config>
      <ethernet xmlns="http://openconfig.net/yang/interfaces/ethernet">
        <switched-vlan xmlns="http://openconfig.net/yang/vlan">
          <config>
            <interface-mode>ACCESS</interface-mode>
            <access-vlan>30</access-vlan>
          </config>
        </switched-vlan>
      </ethernet>
    </interface>
    <interface>
      <name>vlan30</name>
      <config>
        <name>vlan30</name>
        <enabled>true</enabled>
      </config>
      <routed-vlan xmlns="http://openconfig.net/yang/vlan">
        <config>
          <vlan>30</vlan>
        </config>
        <ipv4 xmlns="http://openconfig.net/yang/interfaces/ip">
          <addresses>
            <address>
              <ip>10.0.30.3</ip>
              <config>
                <ip>10.0.30.3</ip>
                <prefix-length>24</prefix-length>
              </config>
            </address>
          </addresses>
        </ipv4>
      </routed-vlan>
    </interface>
  </interfaces>
</config>